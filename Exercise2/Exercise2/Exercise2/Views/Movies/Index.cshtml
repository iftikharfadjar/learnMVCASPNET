
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movie</h2>
    <table>
        <thead>
            <tr>
                <th> Movie </th>
            </tr>
        </thead>

        <tbody>
            @foreach (var movie in Model)
            {
                <tr>
                    <td>@movie.Name </td>
                </tr>
            }
        </tbody>
    </table>

